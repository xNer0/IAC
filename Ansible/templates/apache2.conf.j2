<VirtualHost *:80>
    ServerAdmin webmaster@localhost
    DocumentRoot /var/www/semui.co

    <Directory /var/www/semui.co>
        Options Indexes FollowSymLinks
        AllowOverride None
        Require all granted
    </Directory>

    ErrorLog ${APACHE_LOG_DIR}/error.log
    CustomLog ${APACHE_LOG_DIR}/access.log combined

    Alias /static /var/www/semui.co/static
    <Directory /var/www/semui.co/static>
        Require all granted
    </Directory>

    Alias /media /var/www/semui.co/media
    <Directory /var/www/semui.co/media>
        Require all granted
    </Directory

    <Directory /var/www/semui.co>
        Require all granted
    </Directory>
</VirtualHost>
